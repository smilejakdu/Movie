<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--    style AlertDialog button-->
    <style name="MyAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/colorbase</item>
    </style>
    <!--    progressbar-->
    <style name="ProgressBar">
        <item name="colorAccent">@color/colorbase</item>
        <item name="android:backgroundTint">@color/colorbase</item>
    </style>

    <style name="coll_main_basic_title">
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="coll_expand_title">
        <item name="android:textColor">@color/colorbase</item>
    </style>

</resources>
